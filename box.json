{
  "directories": ["src", "bin","vendor"],
  "compression": "GZ",
  "finder": [
    {
      "name": "*.php",
      "notName": "/LICENSE|.*\\.md|.*\\.dist|Makefile|composer\\.json|composer\\.lock/",
      "exclude": [
        "tests",
        "spec",
        "doc",
        "test",
        "Tests",
        "fixtures",
        "squizlabs",
        "seld",
        "phpstan",
        "phpmd",
        "php-parallel-lint",
        "monolog",
        "magento",
        "webonyx",
        "phpro"
      ],
      "in": "vendor"
    }
  ],
  "blacklist": [
    "squizlabs",
    "seld",
    "phpstan",
    "phpmd",
    "phpunit",
    "php-parallel-lint",
    "monolog",
    "magento",
    "webonyx",
    "phpro"
  ],
  "main": "bin/jeeves",
  "output": "jeeves.phar"
}
